oString() });\n      return updatedGoals;\n    });\n  };\n\n  const addSubitemHandler = () => {\n    console.log('Add handler');\n    console.log(input);\n    setInput({\n      title: 'Add subItem',\n      message: 'Put the name of the action you want to do'\n    })\n    \n  }\n\n  const deleteItemHandler = goalId => {\n    setCourseGoals(prevGoals => {\n      const updatedGoals = prevGoals.filter(goal => goal.id !== goalId);\n      return updatedGoals;\n    });\n  };\n\n  const editItemHandler =  (goalId, inputUser) => {\n    goalIdToChange = goalId;\n    setCourseGoals(prevGoals => {\n      const updatedGoals = prevGoals.filter(goal => goal.id !== goalId);\n      updatedGoals.unshift({ text: inputUser, id: goalId });\n      return updatedGoals;\n    })\n    \n\n  }\n\n  const inputHandler = () => {\n    setInput(null);\n  }\n\n\n\n  let content = (\n    <p style={{ textAlign: 'center' }}>No goals found. Maybe add one?</p>\n  );\n\n  if (courseGoals.length > 0) {\n    content = (\n      <CourseGoalList items={courseGoals} onEditItem={editItemHandler} onDeleteItem={deleteItemHandler} onAddSubItem={addSubitemHandler}/>\n    );\n  }\n\n  return (\n    <Wrapper>\n      {input && <InputModal title={input.title} message={input.message} onConfirm={inputHandler} />}\n      <div>\n        <section id=\"goal-form\">\n          <CourseInput onAddGoal={addGoalHandler} />\n        </section>\n        <section id=\"goals\">\n          {content}\n        </section>\n      </div>\n    </Wrapper>\n  );\n};\n\nexport default App;\n","C:\\Users\\josed\\Documents\\Sofka\\Reto1KataCrudRefactoring\\Front\\src\\components\\Helpers\\Wrapper.js",[],"C:\\Users\\josed\\Documents\\Sofka\\Reto1KataCrudRefactoring\\Front\\src\\components\\CourseGoals\\CourseGoalList\\CourseGoalList.js",[],"C:\\Users\\josed\\Documents\\Sofka\\Reto1KataCrudRefactoring\\Front\\src\\components\\UI\\InputModal\\InputModal.js",[],"C:\\Users\\josed\\Documents\\Sofka\\Reto1KataCrudRefactoring\\Front\\src\\components\\C